<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.news">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".control.MainActivity" />
        <activity android:name=".base.control.BaseActivity" />
        <activity android:name=".control.LogoActivity" />
        <activity android:name=".control.LeadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".control.UserCenterActivity" />
        <activity android:name=".control.NewsContentActivity" />
        <activity android:name=".control.InavtationActivity"></activity>
    </application>

</manifest>